  "version": "0.0.1",
  "scripts": {
    "build:binary": "bun build --compile --minify --sourcemap ./src/index.ts ./src/files/**/*.* --define \"process.env.VERSION=\\\"$(jq -r .version package.json)\\\"\" --outfile ./bin/pf",
